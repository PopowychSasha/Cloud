{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "parser": "@babel/eslint-parser",

  "plugins": ["jsx"],

  "rules": {
    "quotes": ["error", "single"],
    "no-prototype-builtins": ["off", "always"],
    "no-unused-vars": ["off"]
  }
}
